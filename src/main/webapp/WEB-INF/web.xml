<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>EmpReimSys</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/error</location>
  </error-page>
 <servlet>
		<description></description>
		<display-name>FrontController</display-name>
		<servlet-name>FrontController</servlet-name>
		<servlet-class>com.ers.web.FrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>/login</url-pattern>
		<url-pattern>/logout</url-pattern>
		<url-pattern>/employees</url-pattern>
		<url-pattern>/signup</url-pattern>
		<url-pattern>/error</url-pattern>
		<url-pattern>/addExpense</url-pattern>
		<url-pattern>/readExpEmp</url-pattern>
		<url-pattern>/expimage</url-pattern>
		<url-pattern>/editprofile</url-pattern>
		<url-pattern>/readAllExp</url-pattern>
		<url-pattern>/approve</url-pattern>
		<url-pattern>/deny</url-pattern>
		
			</servlet-mapping>
</web-app>